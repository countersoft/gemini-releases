@model List<ProjectSummary.Models.ProjectAttribute>

<div class="admin-datatable-holder">
    <table id="project-attribute-table" class="admin-data-entry" cellpadding="0" cellspacing="0">        
            @foreach (var attribute in Model)
            {
                <tr data-id="@attribute.Attributeid" id="row-@attribute.Attributeid">                  
                    <td class="name">@attribute.Attributename</td>
                    <td>@Html.Raw(attribute.Attributevalue)</td>
                </tr>
            }        
    </table>
</div>